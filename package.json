{
  "scripts": {
    "postinstall": "npm run build:ssr",
    "pre-e2e": "webdriver-manager update --standalone false --gecko false",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "extract": "ngx-translate-extract  -i ./src -o ./src/assets/languages/ar.json ./src/assets/languages/en.json --sort --clean --marker _extract",
    "bundle-report": "npm run build:stats && webpack-bundle-analyzer ./dist/browser/stats-es2015.json",
    "ng": "ng",
    "e2e": "ng e2e",
    "start": "ng serve -o",
    "serve:ssr": "node dist/server/main.js",
    "test": "clear && ng test",
    "test:coverage": "rm -rf ./coverage && ng test --codeCoverage --watch false",
    "lint": "ng lint --fix && stylelint './src/**/*.scss' --fix",
    "build": "ng build --prod",
    "build:ssr": "npm run build && ng run buildozer:server:production",
    "prerender": "ng run buildozer:prerender",
    "dev:ssr": "ng run buildozer:serve-ssr"
  },
  "dependencies": {
    "@angular/animations": "~9.1.4",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "~9.1.4",
    "@angular/compiler": "~9.1.4",
    "@angular/core": "~9.1.4",
    "@angular/forms": "~9.1.4",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "~9.1.4",
    "@angular/platform-browser-dynamic": "~9.1.4",
    "@angular/platform-server": "~9.1.4",
    "@angular/router": "~9.1.4",
    "@angular/service-worker": "~9.1.4",
    "@auth0/angular-jwt": "^4.0.0",
    "@ezzabuzaid/document-storage": "^0.4.6",
    "@ezzabuzaid/ngx-request-options": "0.0.5",
    "@joeattardi/emoji-button": "^2.12.1",
    "@nguniversal/express-engine": "^9.0.0",
    "@ngx-translate/core": "^12.0.0",
    "@types/simple-peer": "^9.6.0",
    "bootstrap": "^4.5.0",
    "core-js": "^3.2.1",
    "express": "^4.15.2",
    "intl-tel-input": "^16.0.8",
    "ngx-gravatar": "^9.0.1",
    "ngx-infinite-scroll": "^9.0.0",
    "ngx-timeago": "^1.0.4",
    "rxjs": "~6.5.4",
    "simple-peer": "^9.7.2",
    "socket.io-client": "^2.3.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.4",
    "@angular/cli": "~9.1.4",
    "@angular/compiler-cli": "~9.1.4",
    "@angular/language-service": "~9.1.4",
    "@biesbjerg/ngx-translate-extract": "^6.0.0",
    "@nguniversal/builders": "^9.0.0",
    "@types/bootstrap": "^4.3.1",
    "@types/express": "^4.17.0",
    "@types/googlemaps": "^3.37.4",
    "@types/jasmine": "~3.5.3",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^13.7.0",
    "@types/socket.io-client": "^1.4.32",
    "@types/stylelint": "^9.10.1",
    "codelyzer": "^5.1.2",
    "domino": "^2.1.4",
    "eslint": "^6.8.0",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.5.2",
    "mock-browser": "^0.92.14",
    "protractor": "^7.0.0",
    "stylelint": "^13.1.0",
    "ts-loader": "^6.0.4",
    "ts-node": "~8.6.2",
    "tslint": "^6.1.2",
    "typescript": "^3.7.5",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run build:ssr && git add ."
    }
  },
